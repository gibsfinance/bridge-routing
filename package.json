{
  "name": "@gibsfinance/frontend",
  "version": "1.6.23",
  "private": true,
  "access": "private",
  "workspaces": [
    "packages/common",
    "packages/bridge-sdk",
    "packages/frontend"
  ],
  "scripts": {
    "version": "node scripts/sync-versions.js",
    "ui:dev": "yarn workspace @gibsfinance/frontend run dev",
    "ui:preview": "yarn workspace @gibsfinance/frontend run preview",
    "ui:build": "yarn workspace @gibsfinance/frontend run build",
    "common:build": "yarn workspace @gibsfinance/common run build",
    "ui:check": "yarn workspace @gibsfinance/frontend run check",
    "bridge-sdk:build": "yarn workspace @gibsfinance/bridge-sdk run build",
    "build": "yarn run common:build && yarn run bridge-sdk:build && yarn run ui:build",
    "dev": "yarn run ui:dev",
    "preview": "yarn run ui:preview",
    "pinata:publish": "pinata-cli -a $PINATA_API_KEY && pinata-cli -u packages/frontend/dist/",
    "check": "yarn run common:build && yarn run bridge-sdk:build && yarn run ui:check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "pinata-upload-cli": "^1.1.0"
  }
}
