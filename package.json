{
  "name": "@gibsfinance/frontend",
  "version": "1.6.10",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/common",
    "packages/bridge-sdk",
    "packages/frontend"
  ],
  "scripts": {
    "version": "node scripts/sync-versions.js",
    "ui:dev": "npm run dev --workspace=packages/frontend",
    "ui:preview": "npm run preview --workspace=packages/frontend",
    "ui:build": "npm run build --workspace=packages/frontend",
    "common:build": "npm run build --workspace=packages/common",
    "ui:check": "npm run check --workspace=packages/frontend",
    "bridge-sdk:build": "npm run build --workspace=packages/bridge-sdk",
    "build": "npm run common:build && npm run bridge-sdk:build && npm run ui:build",
    "dev": "npm run ui:dev",
    "preview": "npm run ui:preview",
    "check": "npm run common:build && npm run bridge-sdk:build && npm run ui:check"
  },
