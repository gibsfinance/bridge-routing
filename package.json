{
  "name": "@gibsfinance/frontend",
  "version": "1.6.22",
  "private": true,
  "access": "private",
  "workspaces": [
    "packages/common",
    "packages/bridge-sdk",
    "packages/frontend"
  ],
  "scripts": {
    "version": "node scripts/sync-versions.js",
    "ui:dev": "yarn run dev --workspace=packages/frontend",
    "ui:preview": "yarn run preview --workspace=packages/frontend",
    "ui:build": "yarn run build --workspace=packages/frontend",
    "common:build": "yarn run build --workspace=packages/common",
    "ui:check": "yarn run check --workspace=packages/frontend",
    "bridge-sdk:build": "yarn run build --workspace=packages/bridge-sdk",
    "build": "yarn run common:build && yarn run bridge-sdk:build && yarn run ui:build",
    "dev": "yarn run ui:dev",
    "preview": "yarn run ui:preview",
    "check": "yarn run common:build && yarn run bridge-sdk:build && yarn run ui:check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
